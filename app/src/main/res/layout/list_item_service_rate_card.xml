<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/_5ssp"
        android:background="@drawable/button_border"
        android:gravity="center"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/service_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/clanpro_medium"
            android:padding="12dp"
            android:text="Service name"
            android:textColor="@color/colorPrimaryText"
            android:textSize="@dimen/text_small"
            android:textStyle="bold"
            tools:ignore="HardcodedText" />


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_5sdp"
            android:background="@drawable/button_border">

            <TextView
                android:id="@+id/Outstation"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/clanpro_medium"
                android:padding="12dp"
                android:text="Outstation"
                android:textColor="@color/colorPrimaryText"
                android:textSize="@dimen/text_small"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />

            <TextView
                android:id="@+id/Outstation_per_minute"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Outstation"
                android:fontFamily="@font/clanpro_medium"
                android:padding="12dp"
                android:textColor="@color/colorPrimaryText"
                android:textSize="@dimen/text_small" />

            <TextView
                android:id="@+id/Outstation_per_km"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Outstation_per_minute"
                android:fontFamily="@font/clanpro_medium"
                android:padding="12dp"
                android:textColor="@color/colorPrimaryText"
                android:textSize="@dimen/text_small" />

            <TextView
                android:id="@+id/Rental"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/clanpro_medium"
                android:layout_below="@+id/Outstation_per_km"
                android:padding="12dp"
                android:text="Rental"
                android:textColor="@color/colorPrimaryText"
                android:textSize="@dimen/text_small"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />

            <TextView
                android:id="@+id/Rental_per_km"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Rental"
                android:fontFamily="@font/clanpro_medium"
                android:padding="12dp"
                android:textColor="@color/colorPrimaryText"
                android:textSize="@dimen/text_small" />

            <TextView
                android:id="@+id/Rental_per_minute"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Rental_per_km"
                android:fontFamily="@font/clanpro_medium"
                android:padding="12dp"
                android:textColor="@color/colorPrimaryText"
                android:textSize="@dimen/text_small" />


            <TextView
                android:id="@+id/Rental_package"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/clanpro_medium"
                android:layout_below="@+id/Rental_per_minute"
                android:padding="12dp"
                android:text="Rental Hour Packages"
                android:textColor="@color/colorPrimaryText"
                android:textSize="@dimen/text_small"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />

            <TextView
                android:id="@+id/package_hour"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Rental_package"
                android:fontFamily="@font/clanpro_medium"
                android:padding="12dp"
                android:textColor="@color/colorPrimaryText"
                android:textSize="@dimen/text_small" />

            <TextView
                android:id="@+id/package_km"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/package_hour"
                android:fontFamily="@font/clanpro_medium"
                android:padding="12dp"
                android:textColor="@color/colorPrimaryText"
                android:textSize="@dimen/text_small" />

        </RelativeLayout>

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_15sdp"
        android:layout_below="@+id/main"
        tools:ignore="MissingConstraints"
        />
</RelativeLayout>